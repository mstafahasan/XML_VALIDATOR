<?xml version="1.0" encoding="UTF-8"?>
<AR-PACKAGE>
  <ELEMENTS>
    <SENDER-RECEIVER-INTERFACE>
      <SHORT-NAME name="Interface1" />
      <DATA-ELEMENTS>
        <VARIABLE-DATA-PROTOTYPE>
          <SHORT-NAME name="Var1" />
          <TYPE-TREF>
            <Array-of-uint8 No_Of_Bytes="8">
              <Random-Values-generated>
                <Data-Range min="0" max="255" />
              </Random-Values-generated>
            </Array-of-uint8>
          </TYPE-TREF>
        </VARIABLE-DATA-PROTOTYPE>
      </DATA-ELEMENTS>
    </SENDER-RECEIVER-INTERFACE>
    <PRE-BUILT-APPLICATION>
      <SHORT-NAME name="App1" />
      <PATH DEST="bin/app1.bin" />
    </PRE-BUILT-APPLICATION>
    <Network-Topology>
      <InterECU_communication>
        <CAN-BUS>
          <SHORT-NAME name="CAN1" />
          <BaudRate value="500000" />
          <INTERFACE-TREF DEST="/Interface1" />
        </CAN-BUS>
        <Eth-Switch>
          <SHORT-NAME name="Eth1" />
          <INTERFACE-TREF DEST="/Interface1" />
        </Eth-Switch>
      </InterECU_communication>
    </Network-Topology>
    <ECUs>
      <SHORT-NAME name="ECU1" />
      <SoCs>
        <SHORT-NAME name="SoC1" />
        <AXI-BUS width="4" frequency="100000000" />
  <ETHERNET-INTERFACE Mode="simulated" />
        <UCIe-INTERFACE Mode="host">
          <D2D_Configuration DestChipletRef="/ECU1/SoC1">
            <LINK_WIDTH value="256" />
            <LINK_FREQUENCY value="250000000" />
            <PROTOCOL_CONV_LATENCY value="6" unit="ns" />
            <CHIPLET_LINK_LATENCY value="2" unit="ns" />
            <LOGICAL_IF_DELAY value="6" unit="ns" />
            <PHYSICAL_IF_DELAY value="5" unit="ns" />
          </D2D_Configuration>
        </UCIe-INTERFACE>
        <Generic_Hardware>
          <SHORT-NAME name="SensorProducer" />
          <Frequency value="1000000" />
          <AXI-Master-Port priority="10" />
          <MEMORY-INTERFACE InternalMemory="true" />
          <INTERNAL-BEHAVIOR>
            <PORTS>
              <P-PORT-PROTOTYPE>
                <SHORT-NAME name="PPort1" />
                <PROVIDED-INTERFACE-TREF DEST="/Interface1" />
              </P-PORT-PROTOTYPE>
            </PORTS>
            <OPERATIONS-SEQUENCE>
              <OPERATION>
                <WRITE>
                  <IREF DEST="/genericHardware-name/pPort-name" />
                </WRITE>
              </OPERATION>
            </OPERATIONS-SEQUENCE>
            <EVENT>
              <TIMING-EVENT>
                <PERIOD value="100" unit="ms" />
              </TIMING-EVENT>
            </EVENT>
          </INTERNAL-BEHAVIOR>
        </Generic_Hardware>
      </SoCs>
    </ECUs>
    <Analysis>
      <SW-Analysis-Enable>
        <ALL-SoCs>Enabled</ALL-SoCs>
      </SW-Analysis-Enable>
      <HW-Analysis-Enable>
        <ALL-SoCs>Enabled</ALL-SoCs>
      </HW-Analysis-Enable>
    </Analysis>
  </ELEMENTS>
</AR-PACKAGE>
