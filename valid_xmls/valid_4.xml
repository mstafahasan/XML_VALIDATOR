<?xml version="1.0" encoding="UTF-8"?>
<AR-PACKAGE>
  <ELEMENTS>
    <SENDER-RECEIVER-INTERFACE>
      <SHORT-NAME name="Interface4" />
      <DATA-ELEMENTS>
        <VARIABLE-DATA-PROTOTYPE>
          <SHORT-NAME name="Var4" />
          <TYPE-TREF>
            <Array-of-float No_Of_Elements="32">
              <Fixed-values-generated Default-Value="50.0" />
            </Array-of-float>
          </TYPE-TREF>
        </VARIABLE-DATA-PROTOTYPE>
      </DATA-ELEMENTS>
    </SENDER-RECEIVER-INTERFACE>
    <PRE-BUILT-APPLICATION>
      <SHORT-NAME name="App4" />
      <PATH DEST="bin/app4.bin" />
    </PRE-BUILT-APPLICATION>
    <Network-Topology>
      <InterECU_communication>
        <CAN-BUS>
          <SHORT-NAME name="CAN4" />
          <BaudRate value="1000000" />
          <INTERFACE-TREF DEST="/Interface4" />
        </CAN-BUS>
        <Eth-Switch>
          <SHORT-NAME name="Eth4" />
          <INTERFACE-TREF DEST="/Interface4" />
        </Eth-Switch>
      </InterECU_communication>
    </Network-Topology>
    <ECUs>
      <SHORT-NAME name="ECU4" />
      <SoCs>
        <SHORT-NAME name="SoC4" />
        <AXI-BUS width="32" frequency="800000000" />
  <ETHERNET-INTERFACE Mode="simulated" />
        <UCIe-INTERFACE Mode="endpoint">
          <D2D_Configuration DestChipletRef="/ECU4/SoC4">
            <LINK_WIDTH value="1024" />
            <LINK_FREQUENCY value="2000000000" />
            <PROTOCOL_CONV_LATENCY value="40" unit="ns" />
            <CHIPLET_LINK_LATENCY value="20" unit="ns" />
            <LOGICAL_IF_DELAY value="16" unit="ns" />
            <PHYSICAL_IF_DELAY value="8" unit="ns" />
          </D2D_Configuration>
        </UCIe-INTERFACE>
        <Generic_Hardware>
          <SHORT-NAME name="Controller" />
          <Frequency value="10000000" />
          <AXI-Master-Port priority="1" />
          <MEMORY-INTERFACE InternalMemory="true" />
          <INTERNAL-BEHAVIOR>
            <PORTS>
              <P-PORT-PROTOTYPE>
                <SHORT-NAME name="PPort4" />
                <PROVIDED-INTERFACE-TREF DEST="/Interface4" />
              </P-PORT-PROTOTYPE>
            </PORTS>
            <OPERATIONS-SEQUENCE>
              <OPERATION>
                <CUSTOM-OPERATION functionPrototype="void custom_func(void)" headerFile="custom.h" includesDir="inc" sourcesDir="src" />
              </OPERATION>
            </OPERATIONS-SEQUENCE>
            <EVENT>
              <TIMING-EVENT>
                <PERIOD value="100" unit="ms" />
              </TIMING-EVENT>
            </EVENT>
          </INTERNAL-BEHAVIOR>
        </Generic_Hardware>
      </SoCs>
    </ECUs>
    <Analysis>
      <SW-Analysis-Enable>
        <ALL-SoCs>Enabled</ALL-SoCs>
      </SW-Analysis-Enable>
      <HW-Analysis-Enable>
        <ALL-SoCs>Enabled</ALL-SoCs>
      </HW-Analysis-Enable>
    </Analysis>
  </ELEMENTS>
</AR-PACKAGE>
