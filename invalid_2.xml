<?xml version="1.0" encoding="UTF-8"?>
<AR-PACKAGE>
  <ELEMENTS>
    <SENDER-RECEIVER-INTERFACE>
      <SHORT-NAME name="Interface2" />
      <DATA-ELEMENTS>
        <VARIABLE-DATA-PROTOTYPE>
          <SHORT-NAME name="Var2" />
          <TYPE-TREF>
            <Array-of-float No_Of_Elements="0"> <!-- Invalid: No_Of_Elements below minInclusive -->
              <Random-Values-generated>
                <Data-Range min="0.0" max="100.0" />
              </Random-Values-generated>
            </Array-of-float>
          </TYPE-TREF>
        </VARIABLE-DATA-PROTOTYPE>
      </DATA-ELEMENTS>
    </SENDER-RECEIVER-INTERFACE>
    <PRE-BUILT-APPLICATION>
      <SHORT-NAME name="App2" />
      <PATH /> <!-- Missing DEST attribute -->
    </PRE-BUILT-APPLICATION>
    <Network-Topology>
      <InterECU_communication>
        <CAN-BUS>
          <SHORT-NAME name="CAN2" />
          <BaudRate value="500000" />
          <!-- Missing INTERFACE-TREF -->
        </CAN-BUS>
        <Eth-Switch>
          <SHORT-NAME name="Eth2" />
          <INTERFACE-TREF DEST="/Interface2" />
        </Eth-Switch>
      </InterECU_communication>
    </Network-Topology>
    <ECUs>
      <SHORT-NAME name="ECU2" />
      <SoCs>
        <SHORT-NAME name="SoC2" />
        <AXI-BUS width="8" frequency="500000" /> <!-- Invalid: frequency below minInclusive -->
        <ETHERNET-INTERFACE Mode="native" />
        <UCIe-INTERFACE Mode="endpoint">
          <D2D_Configuration> <!-- Missing required DestChipletRef attribute -->
            <LINK_WIDTH value="128" />
            <!-- Missing LINK_FREQUENCY -->
            <PROTOCOL_CONV_LATENCY value="10" unit="ns" />
            <CHIPLET_LINK_LATENCY value="5" unit="ns" />
            <LOGICAL_IF_DELAY value="3" unit="ns" />
            <PHYSICAL_IF_DELAY value="2" unit="ns" />
          </D2D_Configuration>
        </UCIe-INTERFACE>
        <Generic_Hardware>
          <SHORT-NAME name="Actuator" />
          <Frequency value="2000000" />
          <AXI-Master-Port priority="300" /> <!-- Invalid: priority above maxInclusive -->
          <MEMORY-INTERFACE InternalMemory="false" />
          <INTERNAL-BEHAVIOR>
            <PORTS>
              <P-PORT-PROTOTYPE>
                <SHORT-NAME name="PPort2" />
                <!-- Missing PROVIDED-INTERFACE-TREF -->
              </P-PORT-PROTOTYPE>
            </PORTS>
            <OPERATIONS-SEQUENCE>
              <OPERATION>
                <READ>
                  <IREF /> <!-- Missing DEST attribute -->
                </READ>
              </OPERATION>
            </OPERATIONS-SEQUENCE>
            <EVENT>
              <DATA-RECEIVED-EVENT>
                <!-- Missing REQUIRED-PORT-TREF -->
              </DATA-RECEIVED-EVENT>
            </EVENT>
          </INTERNAL-BEHAVIOR>
        </Generic_Hardware>
      </SoCs>
    </ECUs>
    <Analysis>
      <SW-Analysis-Enable>
        <!-- Missing ALL-SoCs element -->
      </SW-Analysis-Enable>
      <HW-Analysis-Enable>
        <ALL-SoCs></ALL-SoCs> 
      </HW-Analysis-Enable>
    </Analysis>
  </ELEMENTS>
</AR-PACKAGE>