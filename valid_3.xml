<?xml version="1.0" encoding="UTF-8"?>
<AR-PACKAGE>
  <ELEMENTS>
    <SENDER-RECEIVER-INTERFACE>
      <SHORT-NAME name="Interface3" />
      <DATA-ELEMENTS>
        <VARIABLE-DATA-PROTOTYPE>
          <SHORT-NAME name="Var3" />
          <TYPE-TREF>
            <Array-of-uint8 No_Of_Bytes="32">
              <Fixed-values-generated Default-Value="128" />
            </Array-of-uint8>
          </TYPE-TREF>
        </VARIABLE-DATA-PROTOTYPE>
      </DATA-ELEMENTS>
    </SENDER-RECEIVER-INTERFACE>
    <PRE-BUILT-APPLICATION>
      <SHORT-NAME name="App3" />
      <PATH DEST="bin/app3.bin" />
    </PRE-BUILT-APPLICATION>
    <Network-Topology>
      <InterECU_communication>
        <CAN-BUS>
          <SHORT-NAME name="CAN3" />
          <BaudRate value="250000" />
          <INTERFACE-TREF DEST="/Interface3" />
        </CAN-BUS>
        <Eth-Switch>
          <SHORT-NAME name="Eth3" />
          <INTERFACE-TREF DEST="/Interface3" />
        </Eth-Switch>
      </InterECU_communication>
    </Network-Topology>
    <ECUs>
      <SHORT-NAME name="ECU3" />
      <SoCs>
        <SHORT-NAME name="SoC3" />
        <AXI-BUS width="16" frequency="400000000" />
        <ETHERNET-INTERFACE Mode="simulated" />
        <UCIe-INTERFACE Mode="host">
          <D2D_Configuration DestChipletRef="/ECU3/SoC3">
            <LINK_WIDTH value="512" />
            <LINK_FREQUENCY value="1000000000" />
            <PROTOCOL_CONV_LATENCY value="20" unit="ns" />
            <CHIPLET_LINK_LATENCY value="10" unit="ns" />
            <LOGICAL_IF_DELAY value="8" unit="ns" />
            <PHYSICAL_IF_DELAY value="4" unit="ns" />
          </D2D_Configuration>
        </UCIe-INTERFACE>
        <Generic_Hardware>
          <SHORT-NAME name="Processor" />
          <Frequency value="5000000" />
          <AXI-Master-Port priority="5" />
          <MEMORY-INTERFACE InternalMemory="false" />
          <INTERNAL-BEHAVIOR>
            <PORTS>
              <P-PORT-PROTOTYPE>
                <SHORT-NAME name="PPort3" />
                <PROVIDED-INTERFACE-TREF DEST="/Interface3" />
              </P-PORT-PROTOTYPE>
            </PORTS>
            <OPERATIONS-SEQUENCE>
              <OPERATION>
                <LOAD value="1000" />
              </OPERATION>
            </OPERATIONS-SEQUENCE>
            <EVENT>
              <TRIGGER-EVENT>
                <TRIGGER DEST="/Interface3" />
              </TRIGGER-EVENT>
            </EVENT>
          </INTERNAL-BEHAVIOR>
        </Generic_Hardware>
      </SoCs>
    </ECUs>
    <Analysis>
      <SW-Analysis-Enable>
        <ALL-SoCs>Enabled</ALL-SoCs>
      </SW-Analysis-Enable>
      <HW-Analysis-Enable>
        <ALL-SoCs>Enabled</ALL-SoCs>
      </HW-Analysis-Enable>
    </Analysis>
  </ELEMENTS>
</AR-PACKAGE>
